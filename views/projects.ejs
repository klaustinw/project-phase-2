<!DOCTYPE html>
<html lang='en'>
    <head>
        <%- include('./partials/head') -%>
        <meta charset="UTF-8">
        <link rel="stylesheet" href="index.css">
    </head>
    <body>
        <header>
            <%- include('./partials/header') -%>
        </header>
        <table border="2">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Difficulty</th>
                </tr>
            </thead>
            <tbody>
                <% data.forEach(e => { %> 
                <tr>
                    <td><%= e.name %></td>
                    <td><%= e.difficulty %></td>
                </tr>
                <% }) %>
            </tbody>
        </table>
    </body>
    <%- include('./partials/footer') -%>
</html>